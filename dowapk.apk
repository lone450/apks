--metodo offset
function TBO(FF, H4X, IND)
  local info = gg.getTargetInfo()
  local libDir = info.nativeLibraryDir
  local libPath = libDir .. "/lib" .. FF
  local ranges = gg.getRangesList(libPath)

  -- Verifique se hÃ¡ intervalos disponÃ­veis
  if #ranges == 0 then
    gg.alert("NÃ£o foi possÃ­vel encontrar a biblioteca: " .. libPath)
    return
  end

  -- Encontre o endereÃ§o base da biblioteca especificada
  local baseAddress
  for _, range in pairs(ranges) do
    local values = gg.getValues({
      {address = range.start, flags = gg.TYPE_DWORD}
    })
    
    if values[1].value == 0x464C457F then
      baseAddress = range.start
      break
    end
  end
  
  if not baseAddress then
    gg.alert("EndereÃ§o base nÃ£o encontrado.")
    return
  end
  
  baseAddress = baseAddress + H4X

  local valuesToWrite = {}

  if type(IND) == "number" then
    -- ManipulaÃ§Ã£o de bytes quando IND Ã© um nÃºmero
    for i = 1, IND do
      valuesToWrite[i] = {address = (baseAddress - 1) + i, flags = gg.TYPE_BYTE}
    end
    
    local result = ""
    for i, val in ipairs(gg.getValues(valuesToWrite)) do
      result = result .. string.format("%02X", val.value & 0xFF)
    end
    
    return result
  else
    -- ManipulaÃ§Ã£o de valores hexadecimais quando IND Ã© uma string
    IND:gsub("..", function(hexByte)
      table.insert(valuesToWrite, {
        address = (baseAddress - 1) + #valuesToWrite + 1,
        flags = gg.TYPE_BYTE,
        value = hexByte .. "h"
      })
    end)
    
    gg.setValues(valuesToWrite)
    gg.toast("Valor modificado com sucesso!")
  end
end
